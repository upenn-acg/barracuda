        StopWatchInterface() {};
        virtual ~StopWatchInterface() {};

    public:

        virtual void start() = 0;


        virtual void stop() = 0;


        virtual void reset() = 0;




        virtual float getTime() = 0;



        virtual float getAverageTime() = 0;
