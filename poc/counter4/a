static __inline__ __attribute__((always_inline)) __attribute__((device)) char tex1DLod(texture<char, 0x01, cudaReadModeElementType> t, float x, float level)
{



  int4 v = __itexfetchlod(t, make_float4(x, 0, 0, 0), level);


  return (char)v.x;
}

