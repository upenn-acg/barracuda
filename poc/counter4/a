static const int WARP_SIZE = 32;







enum PROTO_OP
{
    OP_OTHER = 0x00,
    OP_LOAD = 0x01,
    OP_STORE = 0x02,
    OP_ATOMIC = 0x03,
    OP_SYNCTHREADS = 0x04,
    OP_CONVERGE = 0x05,
    OP_ACQUIRE = 0x10,
 OP_ACQ_CTA = OP_ACQUIRE | 0x01,
 OP_ACQ_GL = OP_ACQUIRE | 0x02,
 OP_ACQ_SYS = OP_ACQUIRE | 0x03,
 OP_ATOM_ACQ_CTA = OP_ACQUIRE | 0x01 | 0x04,
 OP_ATOM_ACQ_GL = OP_ACQUIRE | 0x02 | 0x04,
 OP_ATOM_ACQ_SYS = OP_ACQUIRE | 0x03 | 0x04,
    OP_RELEASE = 0x20,
 OP_REL_CTA = OP_RELEASE | 0x01,
 OP_REL_GL = OP_RELEASE | 0x02,
 OP_REL_SYS = OP_RELEASE | 0x03,
 OP_ATOM_REL_CTA = OP_RELEASE | 0x01 | 0x04,
 OP_ATOM_REL_GL = OP_RELEASE | 0x02 | 0x04,
 OP_ATOM_REL_SYS = OP_RELEASE | 0x03 | 0x04,
    OP_ACQREL = 0x30,
 OP_ATOM_ACQREL_CTA = OP_ACQREL | 0x01 | 0x04,
 OP_ATOM_ACQREL_GL = OP_ACQREL | 0x02 | 0x04,
 OP_ATOM_ACQREL_SYS = OP_ACQREL | 0x03 | 0x04,
    PROTO_OP_MAX
};

static const int GLOBAL_FLAG = 0x0100;

typedef uint64_t slimptr_t;

struct PCRecord
{
    uint64_t tid;
    uint32_t active;
    uint16_t op_state;
    uint16_t loc_id;
    slimptr_t address[WARP_SIZE];
};



struct PCHeader
{
    uint32_t read_head;
    uint32_t write_head;
    uint32_t tail;
};
# 4 "devarea.hpp" 2
# 1 "compiler.hpp" 1
       
# 5 "devarea.hpp" 2

class DeviceArea
{
public:
    enum { HEADER_SIZE = 64 };

    __attribute__((host)) __attribute__((device)) __inline__ __attribute__((always_inline)) __attribute((always_inline)) DeviceArea()
    {
    }

    __attribute__((host)) __attribute__((device)) __inline__ __attribute__((always_inline)) __attribute((always_inline)) ~DeviceArea()
    {
    }

    __attribute__((host)) __attribute__((device)) __inline__ __attribute__((always_inline)) __attribute((always_inline)) DeviceArea(void* base, size_t size, unsigned int numq)
    {
        _base = (char*)base;
        _size = size;
        _numq = numq;
        _qbuf_size = (size - 1- numq * HEADER_SIZE) / sizeof(PCRecord) / numq;
    }

    __attribute__((host)) __attribute__((device)) __inline__ __attribute__((always_inline)) __attribute((always_inline)) void* base() const
    {
        return _base;
    }

    __attribute__((host)) __attribute__((device)) __inline__ __attribute__((always_inline)) __attribute((always_inline)) size_t size() const
    {
        return _size;
    }

    __attribute__((host)) __attribute__((device)) __inline__ __attribute__((always_inline)) __attribute((always_inline)) size_t qbuf_size() const
    {
        return _qbuf_size;
    }

    __attribute__((host)) __attribute__((device)) __inline__ __attribute__((always_inline)) __attribute((always_inline)) unsigned int numq() const
    {
        return _numq;
    }

    __attribute__((host)) __attribute__((device)) __inline__ __attribute__((always_inline)) __attribute((always_inline)) PCHeader* header(unsigned int qid) const
    {
        return (PCHeader*)(_base + qid * (HEADER_SIZE + _qbuf_size * sizeof(PCRecord)));
    }

    static __attribute__((host)) __attribute__((device)) __inline__ __attribute__((always_inline)) __attribute((always_inline)) PCRecord* start(PCHeader* hdr)
    {
        return (PCRecord*)(((char*)hdr) + HEADER_SIZE);
    }

private:
    char* _base;
    size_t _size;
    size_t _qbuf_size;
    unsigned int _numq;
};
# 9 "tst6.cu" 2

# 1 "debug.h" 1
       
# 11 "tst6.cu" 2

static const int NUM_BLOCKS = 1;
static const int NUM_THREADS = 1;
static const int NUM_TOTAL = NUM_BLOCKS * NUM_THREADS;

__attribute__((device)) DeviceArea __slimgpu_dev_area;



extern "C" __attribute__((global)) void __slimgpu_init(DeviceArea device_area)
{
    memcpy(&__slimgpu_dev_area, &device_area, sizeof(device_area));

    for(int i = 0; i < __slimgpu_dev_area.numq(); ++ i)
    {
        PCHeader* pcheader = __slimgpu_dev_area.header(i);
        pcheader->read_head = 0;
        pcheader->write_head = 0;
        pcheader->tail = 0;
       
    }

    __threadfence_system();
}

static __attribute__((device)) __inline__ __attribute__((always_inline)) __attribute((always_inline)) unsigned int __ptx_laneid()
{
    unsigned int value;
    asm volatile("mov.u32 %0, %%laneid;" : "=r"(value));
    return value;
}

extern "C" __attribute__((device)) __attribute__((noinline)) uint64_t __slimgpu_gettid(int streamid)
{
    return (((uint64_t)streamid << 48) | ((uint64_t)(blockIdx.x + blockIdx.y * gridDim.x + gridDim.x * gridDim.y * blockIdx.z) << 16) | (((threadIdx.z * (blockDim.x * blockDim.y)) + (threadIdx.y * blockDim.x) + threadIdx.x) >> 5))




                                   ;
}


extern "C" __attribute__((device)) __attribute__((noinline)) void __slimgpu_log(const uint64_t tid, const uint64_t address, const uint32_t op, const uint32_t loc_id)
{
    const unsigned int active = __ballot(1);
    const unsigned int myidx = __ptx_laneid();
    const unsigned int ldridx = __ffs(active) - 1;
    const int qid = blockIdx.x % __slimgpu_dev_area.numq();
    const int size = __slimgpu_dev_area.qbuf_size();
    int pos = 0;
    PCHeader* pcheader = __slimgpu_dev_area.header(qid);
    PCRecord* pcstart = DeviceArea::start(pcheader);
    PCRecord* record = __null;

    if(ldridx == myidx)
    {
        volatile unsigned int* tail = (volatile unsigned int*)&pcheader->tail;
        pos = atomicAdd(&pcheader->write_head, 1);
        while((pos - *tail) >= size)
            __threadfence_system();
    }
    pos = __shfl(pos, ldridx);
    record = pcstart + (pos % size);
   
    record->address[myidx] = address;

    if(ldridx == myidx)
    {
        record->tid = tid;
        record->active = active;
        record->op_state= (__isGlobal((void*)address) ? GLOBAL_FLAG : 0) | op;
        record->loc_id = loc_id;
        while(atomicCAS(&pcheader->read_head, pos, pos + 1) != pos)
            __threadfence();
    }
    __threadfence_system();
}


extern "C" __attribute__((global)) void force_function_linking(uint64_t* tid)
{
    *tid = __slimgpu_gettid(0x1234);
    __slimgpu_log(*tid, __null, OP_SYNCTHREADS, 1);
}


int bla()
{
    uint64_t* x;
    if(0 != cudaMalloc(&x, sizeof(uint64_t)))
    {
        printf("Failed cudaMalloc().\n");
        return 1;
    }
    void* buf;
    int buf_size = 64 * 1000;
    if(0 != cudaMalloc(&buf, buf_size))
    {
        printf("Failed cudaMalloc().\n");
        return 1;
    }
    DeviceArea devarea(buf, buf_size, 2);

    __slimgpu_init<<<1,1>>>(devarea);
    int sync = cudaDeviceSynchronize();
    if(sync != 0)
    {
        printf("%s failed, err=%i\n", "__slimgpu_init", sync);
        return 2;
    }

    force_function_linking<<<1,1>>>(x);
    sync = cudaDeviceSynchronize();
    if(sync != 0)
    {
        printf("Link function failed, err=%i\n", sync);
        return 2;
    }
    printf("PTX stubs tested OK!\n");
    return 0;
}

__attribute__((global)) void tstfun(volatile unsigned int* data, int repeats)
{
    uint64_t tid = __slimgpu_gettid(0);
    int id = ((blockDim.x * blockIdx.x) + threadIdx.x);
    data[id] = 0;
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 2, 2);
    int x1 = 0, x2 = 0, x3 = 0;

    x1 = data[id];
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 1, 3);
    atomicAdd((unsigned int*)&data[id], 1);
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 3, 4);
    __syncthreads();
    __slimgpu_log(tid, 0, 4, 3);
    __threadfence();
    atomicAdd((unsigned int*)&data[id], 1);
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 54, 5);
    __syncthreads();
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 4, 6);
    __threadfence();
    atomicAdd((unsigned int*)&data[id], 1);
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 54, 7);
    __syncthreads();
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 4, 8);
    __threadfence();
    data[id] = 0;
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 34, 8);
    __syncthreads();
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 4, 9);
    data[id] = 0;
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 2, 10);
    __threadfence();
    __syncthreads();
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 4, 11);
    __threadfence();
    x2 = data[id];
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 1, 12);
    __syncthreads();
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 4, 13);
    __threadfence();
    x3 = data[id];
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 1, 14);
    __syncthreads();
    __syncthreads();
    __threadfence();
    atomicExch((unsigned int*)&data[id], 1);
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 38, 15);
    __syncthreads();
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 4, 16);
    __threadfence();
    atomicExch((unsigned int*)&data[id], 1);
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 38, 17);
    __syncthreads();
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 4, 18);
    __threadfence();
    atomicCAS((unsigned int*)&data[id], 0, 1);
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 3, 19);
    __syncthreads();
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 4, 20);
    __threadfence();
    atomicCAS((unsigned int*)&data[id], 0, 1);
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 3, 21);

    if(x1 == 0x112233 || x2 == 0x23929303 || x3 == 0x84938493)
        printf("BLA!");
    __slimgpu_log(tid, (((uint64_t)(uintptr_t)(void*))&data[id]), 5, 22);
}



int main(int argc, char* argv[])
{

    unsigned int* dev_data;
    check ( (cudaMalloc(&dev_data, sizeof(unsigned int) * NUM_TOTAL)), "cudaMalloc(&dev_data, sizeof(unsigned int) * NUM_TOTAL)", "tst6.cu", 208 );
    check ( (cudaMemset(dev_data, 0, sizeof(unsigned int) * NUM_TOTAL)), "cudaMemset(dev_data, 0, sizeof(unsigned int) * NUM_TOTAL)", "tst6.cu", 209 );
    unsigned int* host_data = (unsigned int*)malloc(sizeof(unsigned int) * NUM_TOTAL);

    tstfun<<<NUM_BLOCKS, NUM_THREADS>>>(dev_data, 5);
    check ( (cudaDeviceSynchronize()), "cudaDeviceSynchronize()", "tst6.cu", 213 );
    check ( (cudaMemcpy(host_data, dev_data, sizeof(unsigned int) * NUM_TOTAL, cudaMemcpyDeviceToHost)), "cudaMemcpy(host_data, dev_data, sizeof(unsigned int) * NUM_TOTAL, cudaMemcpyDeviceToHost)", "tst6.cu", 214 );
    printf("Success.\n");

    if(argc == 20920)
        bla();

    return 0;
}
