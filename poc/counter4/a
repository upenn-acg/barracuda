inline float
sdkGetAverageTimerValue(StopWatchInterface **timer_interface)
{

    if (*timer_interface)
    {
        return (*timer_interface)->getAverageTime();
    }
    else
    {
        return 0.0f;
    }
}






inline float
sdkGetTimerValue(StopWatchInterface **timer_interface)
{

    if (*timer_interface)
    {
        return (*timer_interface)->getTime();
    }
    else
    {
        return 0.0f;
    }
}
