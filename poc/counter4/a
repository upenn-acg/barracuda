int4 __itexfetchlodl(texture<T, texType, readMode> t, float4 i, int l,
                     float level, int d = (texType & 0xF))
{
  switch(d) {
  case 1: return __itexfetchlodl1D(t, i, l, level);

  default: return __itexfetchlodl2D(t, i, l, level);
  }
}

